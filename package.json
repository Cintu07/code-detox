{
  "name": "code-detox",
  "version": "1.0.0",
  "description": "CLI tool to detect unused code, dead files, stale components and import rot in JavaScript/TypeScript projects",
  "main": "dist/cli/index.js",
  "type": "module",
  "bin": {
    "code-detox": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cleanup",
    "unused-code",
    "dead-code",
    "code-analysis",
    "ast",
    "refactor",
    "cli",
    "static-analysis",
    "linter",
    "code-quality",
    "javascript",
    "typescript",
    "babel"
  ],
  "author": "Cintu07",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cintu07/code-detox.git"
  },
  "bugs": {
    "url": "https://github.com/Cintu07/code-detox/issues"
  },
  "homepage": "https://github.com/Cintu07/code-detox#readme",
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "ora": "^7.0.1",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.4",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2"
  }
}
